  <script>
(() => {
  class Staticfiles {
    jsonMap = null;
    DEVELOPMENT = false;
    MAX_RETRIES = 3;
    RETRY_DELAY = 500; // milisegundos

    constructor() {
      this.CLOUDFRONT_URL = this.hostname("");
      if (this.CLOUDFRONT_URL.includes("localhost") || this.CLOUDFRONT_URL.includes("192.168.") || this.CLOUDFRONT_URL.includes("127.0.") || this.CLOUDFRONT_URL.includes("172.")) {
        this.DEVELOPMENT = true;
      }
    }

    async wait(ms) {
      return new Promise((resolve) => setTimeout(resolve, ms));
    }

    async fetchWithRetry(url, retries = this.MAX_RETRIES) {
      for (let i = 0; i < retries; i++) {
        try {
          const response = await fetch(url);
          if (!response.ok) throw new Error(`HTTP ${response.status}`);
          return await response.json();
        } catch (error) {
          console.warn(`Fetch attempt ${i + 1} failed: ${error.message}`);
          if (i < retries - 1) await this.wait(this.RETRY_DELAY);
        }
      }
      console.error(`Failed to fetch after ${retries} attempts.`);
      return {}; // Fallback a un objeto vacío
    }

    async initialize() {
      if (!this.DEVELOPMENT) {
        this.jsonMap = await this.fetchWithRetry(`${this.CLOUDFRONT_URL}/static/staticfiles.json`);
      } else {
        // Mock some timeout for development
        console.log("Development mode, calling initialize() and waiting 100ms in Staticfiles class");
        await this.wait(100);
        this.jsonMap = {};
      }
    }

    hostname(value = "") {
      if (value && value.includes("http")) return value;

      window.CLOUDFRONT_URL = "https://d209nv3nexrgbg.cloudfront.net";

      if (
        window.location.host.includes("localhost") ||
        window.location.hostname.startsWith("127.") ||
        window.location.hostname.startsWith("192.168.") ||
        window.location.hostname.startsWith("172.")
      ) {
        window.CLOUDFRONT_URL = window.location.origin;
        return `${window.location.origin}${value}`;
      } else if (window.location.hostname.includes("test.atom")) {
        window.CLOUDFRONT_URL = "https://d2udpoebf3pk0y.cloudfront.net";
      } else if (window.location.hostname.includes("atom")) {
        window.CLOUDFRONT_URL = "https://d209nv3nexrgbg.cloudfront.net";
      }

      return value ? `${window.CLOUDFRONT_URL}/${value}` : window.CLOUDFRONT_URL;
    }

    get(src) {
      // San src (delete starting slashes)
      if (src.startsWith("/")) {
        src = src.substring(1);
      }
      if (!this.jsonMap) {
        console.error("jsonMap is not initialized. Call and await initialize() first.");
        return `${this.CLOUDFRONT_URL}/static/${src}?hash=not-initialized`;
      }
      if (!src) {
        throw new Error("src is required");
      }

      if (this.DEVELOPMENT) {
        return `${window.location.origin}/static/${src}?hash=dev`;
      }

      if (!this.jsonMap.paths || !this.jsonMap.paths[src]) {
        console.warn(`No mapping found for "${src}" in jsonMap, returning input.`);
        return `${this.CLOUDFRONT_URL}/static/${src}?hash=no-paths`; // Devuelve el `src` original con cloudfront
      }

      return `${this.CLOUDFRONT_URL}/static/${this.jsonMap.paths[src]}?hash=${this.jsonMap.hash || "not-found"}`;
    }
  }

  const staticfiles = new Staticfiles();
  window.getstatic = (src) => staticfiles.get(src);
  staticfiles.initialize().then(() => {
    window.staticfiles = staticfiles;
  });
})();
</script>
